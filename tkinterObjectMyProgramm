import tkinter as tk

root = tk.Tk()
FrameOne = tk.Frame()
FrameTwo = tk.Frame()

# Label - надпись на экране
# Button - кнопка
# Entry - поле ввода
#Text - большое поле для текста
# Frame - окно для группировки объектов
label = tk.Label(master=FrameOne, text='Первое Приложение', foreground = 'white', background='black', font=('Roboto', 12))
label.pack()
# кнопка
button = tk.Button(master=FrameOne, text='Нажми на меня!', width=25, height=5, fg = 'green', bg='red')
button.pack()
# создание поля для ввода текста
labelName = tk.Label(master=FrameTwo, text='Имя')
entry = tk.Entry(master=FrameTwo)
labelName.pack()
#LabelName.delete(0, tk.END) # удаление элементов
#Label.Name.insert(0,"ИВАН") # вставка текста
#entry.get() # получать значение для текста
entry.pack()
FrameTwo.pack()
FrameOne.pack()



root.mainloop()
